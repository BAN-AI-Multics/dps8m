	name	cmpc_pgbnd
	entry	cmpc_pgbnd

cmpc_pgbnd:	push
	eppbp	ap|2,*	"get ptr to temp seg
	eppbp	bp|0,*
	eppbb	bp|0,*
	adwpbp	1015,du
	adwpbp	2048,du
	mlr	(),(pr),fill(040)
	zero
	desc9a	bp|0,40

	call 	test_cpu$deactivate_tempseg     "do it to the tempseg

	eax6	2
	a9bd	bp|0,x6
	lxl7	38,dl
	cmpc	(pr,rl),(pr),fill(040)
	desc9a	bp|0,x7
	desc9a	bb|0,0
	tnz	fault
	return

fault:	oct	0	"ZOP
	end
