  option  Multics

    name    Bug_B_Gone_1    "
    
    entry  _main_BBG_1      " 1st bug regression module ...

    segref	UtilsA, ioa 
    
    segdef  TRA1

    "
    " we'll store stuff on our stack frame ...
    "
    temp    saveX2      " for saving x2 for subroutine call(s)


        even
L1:     bss     1
L2:     bss     1
SDBR:   bss     2


        even
LAQ:
LA:     bss     1
LQ:     bss     1



_main_BBG_1:
    push
    
    sdbr    SDBR
    
    tsp2    ioa
    acc     /BBG-1 ...\r\n/

    tsx2    ORSQTest    " ORSQ + R/M/W Test ...
    
    tsx2    TRAtest     " tra seg|0,x0 doesn't work in 20184

    tsx2    EPPtest

    return

    "
    " ORSQ may have problems with the new address calculations
    "

    temp    _ortest     " operand storage for ORSQ test

ORSQTest:
    stx2    saveX2      " save return address

    ldq     =0
    orsq    _ortest
    
    lda     _ortest
    sta     LA
    
    tsp2    ioa
    acc     /ORSQ test 0 ... ^n (Should be 0)\r\n/
    nop     LA

    ldq     =o1234567
    orsq    _ortest
    
    lda     _ortest
    sta     LA
    
    tsp2    ioa
    acc     /ORSQ test 1 ... ^n (Should be 1234567)\r\n/
    nop     LA

    ldq     =0
    orsq    _ortest

    lda     _ortest
    sta     LA
    
    tsp2    ioa
    acc     /ORSQ test 2 ... ^n (Should be 1234567)\r\n/
    nop     LA

    ldq     =o3000
    orsq    _ortest

    lda     _ortest
    sta     LA
    
    tsp2    ioa
    acc     /ORSQ test 3 ... ^n (Should be 1237567)\r\n/
    nop     LA

    ldq     =o6543210
    orsq    _ortest

    lda     _ortest
    sta     LA
    
    tsp2    ioa
    acc     /ORSQ test 4 ... ^n (Should be 7777777)\r\n/
    nop     LA

    tsp2    ioa
    acc     /ORSQ bug >squished<\r\n/

    ldx2    saveX2      " restore x2
    tra     0,2         " ... and return

"
" to see if certain TRA work ...
"

TRAtest:
    stx2    saveX2      " save return address

    tsp2    ioa
    acc     /TRA test ... trying 'tra seg|0,x0'...\r\n/

    epp0    TRAvec      " &TRAvec => PR0
    eax0    1

    tra     pr0|0, x0

TRAvec:
    tra     TRATest_fail
    tra     TRATest_pass

TRATest_fail:
    ldq     =1029384756
    stq     LQ

    tsp2    ioa
    acc     /TRA test 0 ... Q=^d... (fail)\r\n/
    nop     LQ

    tra     TRA0

TRATest_pass:
    ldq     =1726354
    stq     LQ

    tsp2    ioa
    acc     /TRA test 0 ... Q=^d... (pass)\r\n/
    nop     LQ

    tra     TRA0

TRA0:

    "
    " If we get here TRA test 0 worked ...
    "

TRA1:
    tsp2    ioa
    acc     /TRA test 1 ... back frpm TestBugsB...\r\n/

    ldx2    saveX2      " restore x2
    tra     0,2         " ... and return

"
" to test EPP functionality ...
"

EPPtest:
    stx2    saveX2      " save return address

    tsp2    ioa
    acc     /EPP test 1 ...\r\n/


    ldx2    saveX2      " restore x2
    tra     0,2         " ... and return

EPPtest_0:

    even

l43170:
    ITS 9, 16

    ITS     0400, 012345

	oct	000400000043
	oct	000000000000

 