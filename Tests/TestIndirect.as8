    nop     Date+1

   " sprpap      g
   " sprp4       f
   " short_call  TAM_main
   " lprp4       f
   " lprpap      g

    call    P(ap|0)

    "call    TAM_main(ap|0)
    call T(ap|0)

    "
    " dink with a few fp numbers in TAM$
    "
    fld     pi
    emcall  6

    fld     E
    emcall  6

    fmp     pi  " pi * e = 8.5397341
    emcall  6

    call    P(ap|0)     " TAM print (again)
    
    "
    " Now lets play with some indirection tests ...
    "
    
    link    TIND_main, <TIND>|[_main_2]
    call    TIND_main (ap|0)




    tsp2	ioa
    acc     /T-H-A-T-S all folks (from 0|main) !!! /

	dis
    

	even

g:  bss 1
f:  bss 1

