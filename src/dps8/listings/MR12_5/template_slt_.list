                                    26  " This "program" generates the slt, name_table, dseg and unpaged page tables
                                    27  " corresponding to the collection 0 segments.  All of this is done through 
                                    28  " three main macros.  
                                    29  
                                    30  " seg starts the declaration of a segment.  It takes two arguments.  One is
                                    31  " the segment name.  The second is either "sup" or "init", specifying whether
                                    32  " this is a supervisor (permanent) or initialization segment.
                                    33  
                                    34  " paged_seg ends the declaration of a segment.  It takes three arguments.  The
                                    35  " first is the repeat of the segment name.  The second is either "init" or 
                                    36  " "sup", specifying whether the page table is permanent (corresponding to a
                                    37  " "unpaged" supervisor segment) or is for initialization only.  The third 
                                    38  " argument is either "norm" or "abs_seg".  A "norm" type is given a page table
                                    39  " and is allocated memory according to the size of the segment.  An "abs_seg"
                                    40  " type uses the max_length to determine the page table size (but cur_length to
                                    41  " determine memory allocation).
                                    42  
                                    43  " unpaged_seg ends the description of a segment whose sdw will specify it as
                                    44  " unpaged.  The second argument here is ignored.
                                    45  
                                    46  " Between the seg and either the unpaged_seg or the paged_seg calls appears 
                                    47  " various calls that set attributes of the segment.
                                    48  
                                    49  	macro	seg
                              1     50  
                              2     51  " start the description of a segment
                              3     52  
                              4     53  	set	s.1,0
                              5     54  	set	s.2,0
                              6     55  &=&2,sup&[
                              7     56  
                              8     57  " use low segment nos for supervisor (permanent) segments
                              9     58  
                             10     59  	set	sup_segno,sup_segno+1
                             11     60  	set	s.segno,sup_segno
                             12     61  &;
                             13     62  " initialization segments use high segnos
                             14     63  
                             15     64  	set	init_segno,init_segno+1
                             16     65  	set	s.segno,init_segno
                             17     66  	set	s.2,s.2+slte_uns.init_seg
                             18     67  &]
                             19     68  	equ	&1_seg_num,s.segno
                             20     69  
                             21     70  	use	names
                             22     71  
                             23     72  " We just set one name for these template_slt_ segments.
                             24     73  
                             25     74  	dec	1		" count
                             26     75  	vfd	18/0,1/0,5/0,12/s.segno
                             27     76  	aci	"&1",32
                             28     77  
                             29     78  " Set up the defaults for this segment.  These are overridden by other macro
                             30     79  " calls.
                             31     80  
                             32     81  	set	s.path,0
                             33     82  	set	s.rings,0
                             34     83  	set	s.max_length,0
                             35     84  	set	s.cur_length,0
                             36     85  	set	s.absloc,0
                             37     86  	set	s.access,0
                             38     87  	set	sf.abs_seg,0
                             39     88  
                             40     89  	set	seg_index,seg_index+1
                             41     90  	equ	&1_index,seg_index
                             42     91  	&end
